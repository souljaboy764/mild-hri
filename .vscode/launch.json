    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                        "--latent-dim",     "3", 
                        "--ssm-components", "5",
                        "--beta",           "0.005", 
                        "--variant",        "1", 
                        "--grad-clip",      "0.5", 
                        "--lr",             "5e-4", 
                        "--cov-reg",        "1e-4",
                        "--downsample",     "1",
                        "--window-size",    "40",
                        "--mce-samples",    "10",
                        "--dataset",        "buetepage",
                        "--model",          "VAE",
                    ],
            "justMyCode": false
        }
    ]
}